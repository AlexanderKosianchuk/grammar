{
  "name": "QarSyncManager",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "run-android": "react-native run-android && node node_modules/react-native/local-cli/cli.js start",
    "log-android": "react-native log-android",
    "build": "(cd android/ && ./gradlew assembleDebug)",
    "get-js": "curl \"localhost:8081/index.android.bundle?platform=android&dev=false&minify=true\" -o \"android/app/src/main/assets/index.android.bundle\"",
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "bundle-android": "react-native bundle --platform android --dev false --entry-file index.android.js --bundle-output android/app/src/main/assets/index.android.bundle --sourcemap-output android/app/src/main/assets/index.android.map --assets-dest android/app/src/main/res/",
    "test": "jest"
  },
  "dependencies": {
    "base-64": "^0.1.0",
    "react": "15.4.2",
    "react-native": "0.42.3",
    "react-native-easy-grid": "^0.1.8",
    "react-native-icons": "^0.4.0",
    "react-native-redux-router": "^1.0.7",
    "react-native-vector-icons": "^4.0.1",
    "react-redux": "^5.0.4",
    "redux": "^3.6.0"
  },
  "devDependencies": {
    "babel-jest": "19.0.0",
    "babel-preset-react-native": "1.9.1",
    "jest": "19.0.2",
    "react-test-renderer": "15.4.2"
  },
  "jest": {
    "preset": "react-native"
  }
}
